#!/bin/bash
#SBATCH --nodes=9
#SBATCH --time=02:00:00
#SBATCH --job-name=job3
#SBATCH --mail-user=gomes@berkeley.edu
#SBATCH --mail-type=ALL,TIME_LIMIT
#SBATCH --constraint=haswell
#SBATCH --qos=regular
#SBATCH --output=my_job.o%j
mpiexec -c 1 -n 8 java -cp $OTMSIMJAR:$OTMMPIHOME/lib/*:$OTMMPIHOME/out_mpijavac runner/RunnerMPI $HOME/code/otm-mpi-experiments/split_files/100/8/100_8 0 2 1000 false &
